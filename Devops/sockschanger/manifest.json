{
	"manifest_version": 2,
	"name": "sockschanger",
	"version": "1.0.13",
	"description": "Use socks5 connection.",
	"permissions": [
		"proxy",
		"<all_urls>",
		"webRequest",
		"webRequestBlocking"
	],
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"applications": {
		"gecko": {
			"strict_min_version": "91.1.0",
			"id": "sockschanger@local"
		}
	}
}